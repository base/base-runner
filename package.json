    "base-config-process": "^0.1.4",
    "base-project": "^0.2.1",
    "debug": "^2.2.0",
    "fs-exists-sync": "^0.1.0",
    "lazy-cache": "^2.0.1",
    "liftoff": "^2.2.1",
    "log-utils": "^0.1.4",
    "mixin-deep": "^1.1.3",
    "os-homedir": "^1.0.1"
  },
  "devDependencies": {
    "base": "^0.8.1",
    "base-generators": "^0.3.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^2.0.0",
    "gulp-format-md": "^0.1.9",
