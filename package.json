{
  "name": "base-runner",
  "description": "Orchestrate multiple instances of base-methods at once.",
  "version": "0.9.0",
  "homepage": "https://github.com/node-base/base-runner",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "node-base/base-runner",
  "bugs": {
    "url": "https://github.com/node-base/base-runner/issues"
  },
  "license": "MIT",
  "files": [
    "index.js",
    "utils.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "base-cli-process": "^0.1.19",
    "base-config-process": "^0.1.9",
    "base-project": "^0.2.1",
    "debug": "^2.6.8",
    "homedir-polyfill": "^1.0.1",
    "liftoff": "^2.2.4",
    "log-utils": "^0.2.1",
    "mixin-deep": "^1.2.0"
  },
  "devDependencies": {
    "base": "^0.13.0",
    "base-generators": "^0.4.6",
    "mocha": "^3.4.2",
    "minimist": "^1.2.0",
    "gulp": "^3.9.1",
    "gulp-mocha": "^3.0.1",
    "gulp-istanbul": "^1.1.1",
    "gulp-eslint": "^3.0.1",
    "gulp-unused": "^0.2.1",
    "gulp-format-md": "^0.1.12"
  },
  "keywords": [
    "base",
    "baseplugin",
    "plugin",
    "runner"
  ],
  "verb": {
    "plugins": [
      "gulp-format-md"
    ],
    "reflinks": [
      "base",
      "liftoff",
      "verb",
      "verb-readme-generator"
    ],
    "related": {
      "list": [
        "base",
        "base-generators",
        "base-options",
        "base-plugins",
        "base-store",
        "base-tasks"
      ]
    },
    "toc": false,
    "run": true,
    "layout": "default",
    "lint": {
      "reflinks": true
    },
    "tasks": [
      "readme"
    ]
  }
}
